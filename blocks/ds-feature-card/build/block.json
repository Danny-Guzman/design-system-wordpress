{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "caweb/ds-feature-card",
  "version": "1.0.0",
  "title": "Feature Card",
  "category": "cagov-design-system",
  "icon": "format-aside",
  "description": "The feature card highlights an action you want people to take or important information on the homepage. It is the first thing a visitor sees.",
  "attributes": {
    "title": {
      "type": "string"
    },
    "mediaID": {
      "type": "number"
    },
    "mediaURL": {
      "type": "string"
    },
    "mediaAlt": {
      "type": "string"
    },
    "mediaWidth": {
      "type": "string"
    },
    "mediaHeight": {
      "type": "string"
    }
  },
  "example": {
    "attributes": {
      "title": "Write titleâ€¦",
      "buttontext": "Register",
      "buttonurl": "https://example.com",
      "mediaURL": "http://www.fillmurray.com/750/500",
      "mediaAlt": "Image Description",
      "mediaWidth": "750",
      "mediaHeight": "500"
    }
  },
  "supports": {
    "html": true
  },
  "textdomain": "ds-feature-card",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./frontend.js",
  "render": "file:./render.php"
}