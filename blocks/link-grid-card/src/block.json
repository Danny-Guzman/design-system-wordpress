{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "caweb/link-grid-card",
	"version": "1.0.0",
	"title": "Link Grid Card",
	"category": "cagov-design-system",
	"icon": "format-aside",
	"description": "The link grid organizes links that function as calls to action (CTAs) or highlight topics.",
	"parent": ["caweb/ds-link-grid"],
	"attributes": {
		"linkText":{
			"type": "string"
		},
		"linkInfo":{
			"type": "object"
		}
	},
	"example": {
		"attributes": {
			"linkText": "Link Text"
		}
	},
	"supports": {
		"html": true
	},
	"textdomain": "link-grid-card",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./frontend.js",
	"render": "file:./render.php"
}
